@model List<MyPortfolio.DAL.Entities.Skill>
<div class="col-six tab-full right">
    <h3>Programlama Dilleri & Yetenekler</h3>

    <ul class="skill-bars">
        @foreach(var item in Model)
        {
            var value = "progress percent" + @item.Value;
            <li>
                <div class="@value"><span>%@item.Value</span></div>
                <strong>@item.Title</strong>
            </li>
        }
    </ul>
</div>